# Development Environment Variables
# Backend Configuration

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Google Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# GitHub API (for repository access)
GH_API_KEY=your_github_token_here

# AI Detection APIs (Optional)
CODEQUIRY_API_KEY=your_codequiry_key_here
COPYLEAKS_KEY=your_copyleaks_key_here
HF_API_KEY=your_huggingface_key_here

# Application Settings
APP_ENV=development
ENVIRONMENT=development
LOG_LEVEL=debug
DEBUG=true
REPORT_DIR=reports
PORT=8000

# Embedding Configuration
EMBEDDING_PROVIDER=sentence-transformers
EMBEDDING_MODEL=all-MiniLM-L6-v2
DISABLE_EXTERNAL_LLM=false
HTTP_TIMEOUT=30

# CORS Settings (development - allows frontend on port 8080)
CORS_ORIGINS=http://localhost:8080,http://localhost:5173,http://127.0.0.1:8080,http://127.0.0.1:5173
